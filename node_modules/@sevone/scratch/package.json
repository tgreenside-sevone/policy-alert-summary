{
  "name": "@sevone/scratch",
  "version": "1.71.0",
  "main": "dist/scratch.cjs.js",
  "module": "dist/scratch.esm.js",
  "types": "./dist/types/index.d.ts",
  "repository": "https://src.sevone.com/eng/UI/scratch.git",
  "author": "Unidonks <unidonks@sevone.com>",
  "license": "MIT",
  "files": [
    "dist",
    "assets"
  ],
  "scripts": {
    "lint": "eslint --ext .js,.jsx,.ts,.tsx ./",
    "type": "tsc --noEmit",
    "clean": "rimraf dist .docz",
    "build": "cross-env NODE_ENV=production rollup -c",
    "build:types": "tsc --emitDeclarationOnly",
    "build:docs": "docz build",
    "build:changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "prepublishOnly": "npm-run-all clean build:changelog --parallel build build:types build:docs",
    "test": "jest",
    "docz:dev": "docz dev",
    "dev": "npm-run-all clean docz:dev"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.5",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@svgr/rollup": "^5.3.1",
    "@types/classnames": "^2.2.9",
    "@types/lodash-es": "^4.17.3",
    "@types/react": "^16.9.2",
    "@types/react-select": "2.0.19",
    "@types/react-sortable-hoc": "^0.6.5",
    "@types/react-svg-inline": "^2.1.1",
    "@types/react-tether": "^0.5.5",
    "@types/tinycolor2": "^1.4.2",
    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "@typescript-eslint/parser": "^2.28.0",
    "babel-eslint": "^10.1.0",
    "conventional-changelog-cli": "^2.0.23",
    "cross-env": "^7.0.2",
    "docz": "^2.0.0-rc.41",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^6.8.0",
    "eslint-config-insight-ui": "^2.0.0-beta.2",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.17.0",
    "eslint-plugin-react": "^7.19.0",
    "gatsby-plugin-react-svg": "^3.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "npm-run-all": "^4.1.5",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "rimraf": "^3.0.0",
    "rollup": "^2.6.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.2",
    "rollup-plugin-postcss": "^2.8.2",
    "scheduler": "0.14.0",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.29",
    "@fortawesome/free-regular-svg-icons": "^5.13.1",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@fortawesome/react-fontawesome": "^0.1.9",
    "@types/react-measure": "^2.0.6",
    "classnames": "^2.2.6",
    "date-fns": "^2.2.1",
    "lodash-es": "^4.17.15",
    "memoize-one": "^5.1.1",
    "react-animate-height": "^2.0.7",
    "react-measure": "2.1.2",
    "react-select": "^2.4.4",
    "react-sortable-hoc": "^1.10.1",
    "react-svg-inline": "^2.1.1",
    "react-tether": "^2.0.4",
    "react-virtuoso": "^0.16.7",
    "tinycolor2": "^1.4.1"
  },
  "peerDependencies": {
    "react": "^16.3.0",
    "react-dom": "^16.3.0"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/scripts/test-setup.js",
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy",
      "\\.(gif|ttf|eot|svg|jpg|png)$": "<rootDir>/__mocks__/file-mock.js"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!lodash-es)"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ]
  }
}
